<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/css/bootstrap.min.css"
        integrity="sha384-xOolHFLEh07PJGoPkLv1IbcEPTNtaed2xpHsD9ESMhqIYd0nLMwNLD69Npy4HI+N" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/jquery@3.5.1/dist/jquery.slim.min.js"
        integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"
        integrity="sha384-9/reFTGAW83EW2RDu2S0VKaIzap3H66lZH81PoYlFhbGU+6BZp6G7niu735Sk7lN"
        crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.2/dist/js/bootstrap.min.js"
        integrity="sha384-+sLIOodYLS7CIrQpBjl+C7nPvqq+FbNUBDunl/OZv93DB7Ln/533i8e/mZXLi/P+"
        crossorigin="anonymous"></script>
</head>

<body>
    <button class="btn btn-primary my-2" id="add_ThamGia">add</button>
    <% thamgias.forEach(function(thamgia) { %>
        <div class="card">
            <div class="card-body">
                <div class="title">
                    <h5 class="card-title">
                        <%= thamgia.maTV %>
                            <%= thamgia.maHD %>
                                <%= thamgia.ngayGioDangKy %>
                                    <%= thamgia.diemTruongDoan %>
                                        <%= thamgia.diemTieuChi1 %>
                                            <%= thamgia.diemTieuChi2 %>
                                                <%= thamgia.diemTieuChi3 %>
                                                    <%= thamgia.nhanXetKhac %>


                    </h5>
                </div>
            </div>
        </div>
        <!-- xóa -->
        <form action="/api/thamgias/delete" method="POST">
            <input class="d-none" value="<%= thamgia._id %>" name="id">
            <button class="del btn btn-danger">delete</button>
        </form>
        <% }) %>
            <!-- thêm  -->
            <div class="form_add-thamgia container mb-5" style="display: none; position: pixed; ">
                <form action="/api/thamgias" method="POST">
                    <div class="form-group">
                        <label for="exampleInputEmail1">maTV</label>
                        <input type="name" class="form-control" id="exampleInputEmail1" name="maTV" placeholder="maTV">
                        <label for="exampleInputEmail1">maHD</label>
                        <input type="name" class="form-control" id="exampleInputEmail1" name="maHD" placeholder="maHD">
                        <label for="exampleInputEmail1">ngayGioDangKy</label>
                        <input type="date" class="form-control" id="exampleInputEmail1" name="ngayGioDangKy"
                            placeholder="ngayGioDangKy">
                        <label for="exampleInputEmail1">diemTruongDoan</label>
                        <input type="number" class="form-control" id="exampleInputEmail1" name="diemTruongDoan"
                            placeholder="diemTruongDoan">
                        <label for="exampleInputEmail1">diemTieuChi1</label>
                        <input type="number" class="form-control" id="exampleInputEmail1" name="diemTieuChi1"
                            placeholder="diemTieuChi1">
                        <label for="exampleInputEmail1">diemTieuChi2</label>
                        <input type="number" class="form-control" id="exampleInputEmail1" name="diemTieuChi2"
                            placeholder="diemTieuChi2">
                        <label for="exampleInputEmail1">diemTieuChi3</label>
                        <input type="number" class="form-control" id="exampleInputEmail1" name="diemTieuChi3"
                            placeholder="diemTieuChi3">
                        <label for="exampleInputEmail1">nhanXetKhac</label>
                        <input type="name" class="form-control" id="exampleInputEmail1" name="nhanXetKhac"
                            placeholder="nhanXetKhac">
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                    <button type="submit" class="btn btn-primary" id="close_form-ThanhVien">close</button>
                </form>
            </div>
            <script>
                $("#add_ThamGia").click(function (e) {
                    e.preventDefault();
                    $(".form_add-thamgia").toggle();
                })
                $("#update_ThanhVien").click(function (e) {
                    e.preventDefault();
                    $(".form_update-ThanhVien").toggle();
                })

                $("#close_form-ThanhVien").click(function (e) {
                    e.preventDefault();
                    $(".form_add-ThanhVien").hide();
                })
                $("#close_form-ThanhVien").click(function (e) {
                    e.preventDefault();
                    $(".form_update-ThanhVien").hide();
                })
            </script>

</body>

</html>